int max (int a, int b, int c) {
    var i = 0;
    if (a >= b & a >= c) {
        i = a;
    } else if (b >= a & b >= c) {
        i = b;
    } else {
        i = c;
    }
    return i;
}

int program (int argc, string[] argv) {
    var a1 = 11;
    var a2 = 22; 
    var a3 = 26;
    return max (a1, a2, a3);
}
